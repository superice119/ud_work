
config wifi-device 'radio0'
	option type 'ralink'
	option variant 'mt7628'
	option hwmode '11g'
	option htmode 'HT40'
	option channel 'auto'
	option country 'CN'
	option region '1'
	option disabled '0'
	option linkit_mode 'apsta'

config wifi-iface 'ap'
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option ifname 'ra0'
	option encryption 'none'
	option ssid 'RisingHF_4F2867'

config wifi-iface 'sta'
	option device 'radio0'
	option mode 'sta'
	option ifname 'apcli0'
	option network 'wwan'
	option encryption 'psk2'
	option ssid 'HUAWEI-4C5F'
	option key '87995629'

